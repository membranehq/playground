{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n opencode,next -c blue,green \"npm run opencode-server\" \"next dev --turbopack\"",
    "dev:all": "concurrently -n opencode,next,inngest -c blue,green,yellow \"npm run opencode-server\" \"next dev --turbopack\" \"npm run inngest:dev\"",
    "dev:next": "next dev --turbopack",
    "dev:workflows": "concurrently -n next,inngest -c green,yellow \"next dev --turbopack\" \"npm run inngest:dev\"",
    "inngest:dev": "npx --ignore-scripts=false inngest-cli@latest dev",
    "build": "next build",
    "build:server": "echo 'Server is TypeScript, no build needed'",
    "start": "next start",
    "start:server": "npx tsx scripts/opencode-server.mts",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "typecheck": "npx tsc --noEmit",
    "opencode-server": "npx tsx scripts/opencode-server.mts"
  },
